mixin newsElement(item)    .news-list-item        .news-list-item-content            a(href="#").news-list-item-content__title=item.name            .news-list-item-content__description=item.description        .news-list-item-date            .news-list-item-date-dm=item.date.dm            .news-list-item-date-y=item.date.y